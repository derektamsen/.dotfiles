# Git configuration file
#

[hub]
	protocol = https
[user]
	name = AUTHORNAME
	email = AUTHOREMAIL
	signingkey = AUTHORGPGKEYID
[alias]
	co = checkout
[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[core]
	excludesfile = ~/.gitignore_global
	protectHFS = true
	protectNTFS = true
[push]
	default = simple
[receive]
	fsckObjects = true
[commit]
	gpgsign = true
[pull]
	rebase = true
