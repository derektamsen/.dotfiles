# Git configuration file
#

[hub]
	protocol = https
[user]
	name = AUTHORNAME
	email = AUTHOREMAIL
[alias]
	co = checkout
[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[core]
	excludesfile = ~/.gitignore_global
	protectHFS = true
	protectNTFS = true
[credential]
	helper = osxkeychain
[push]
	default = simple
[receive]
	fsckObjects = true
